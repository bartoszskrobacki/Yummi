---
export const prerender = false
import Layout from '@layouts/Layout.astro'
import FourSomeBanner from '@components/banners/FourSomeBanner.astro'
import { MainBanner } from '@components/banners/MainBanner.tsx'
import InfoBanner from '@components/info/InfoBanner.astro'
import TripleBanner from '@components/info/TripleBanner.astro'
import MiotInfo from '@components/info/MiotInfo.astro'

export interface FoodMenuItem {
	name: string
	price: string
	description?: string
}

export interface FoodCategory {
	contentTypeId: string
	name: string
	fields: FoodMenuItem[]
}
---

<Layout title="Next to Heaven FCI">
	<MainBanner client:idle />
	<TripleBanner />
	<InfoBanner />
	<FourSomeBanner />
	<MiotInfo/>
</Layout>
